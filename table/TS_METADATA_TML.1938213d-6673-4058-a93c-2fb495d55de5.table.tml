guid: 1938213d-6673-4058-a93c-2fb495d55de5
table:
  name: TS_METADATA_TML
  db: GTM_DB
  schema: CS_TOOLS
  db_table: TS_METADATA_TML
  connection:
    name: CS Tools
    fqn: 075ba586-76a3-4e38-8228-8ed20aecd990
  columns:
  - name: CLUSTER_GUID
    db_column_name: CLUSTER_GUID
    properties:
      column_type: ATTRIBUTE
