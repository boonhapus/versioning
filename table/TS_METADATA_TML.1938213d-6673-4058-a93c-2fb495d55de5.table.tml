  joins_with:
  - name: TS_METADATA_TML_to_TS_CLUSTER
    destination:
      name: TS_CLUSTER
      fqn: 71bfdc02-2147-4105-a052-bea08770f2b2
    "on": "[TS_METADATA_TML::CLUSTER_GUID] = [TS_CLUSTER::Cluster GUID]"
    type: LEFT_OUTER
  - name: TS_METADATA_TML_to_TS_METADATA_OBJECT
    destination:
      name: TS_METADATA_OBJECT
      fqn: 8d76f789-b0f1-4d1b-a782-81f52add6b45
    "on": "[TS_METADATA_TML::CLUSTER_GUID] = [TS_METADATA_OBJECT::CLUSTER_GUID] AND [TS_METADATA_TML::OBJECT_GUID] = [TS_METADATA_OBJECT::Object GUID] AND [TS_METADATA_TML::ORG_ID] = [TS_METADATA_OBJECT::ORG_ID]"
    type: LEFT_OUTER
  - name: TS_METADATA_TML_to_TS_ORG
    destination:
      name: TS_ORG
      fqn: bb641bca-b8f3-4b96-a159-5a8632e50a45
    "on": "[TS_METADATA_TML::ORG_ID] = [TS_ORG::Org ID] AND [TS_METADATA_TML::CLUSTER_GUID] = [TS_ORG::CLUSTER_GUID]"
    type: LEFT_OUTER
  - name: TS_METADATA_TML_to_TS_USER
    destination:
      name: TS_USER
      fqn: 143ff846-08bb-40cc-a7db-d50e8f587983
    "on": "[TS_METADATA_TML::CLUSTER_GUID] = [TS_USER::CLUSTER_GUID] AND [TS_METADATA_TML::AUTHOR_GUID] = [TS_USER::User GUID]"
    type: LEFT_OUTER
  properties:
    sage_config:
      is_sage_enabled: false
